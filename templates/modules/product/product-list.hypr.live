<div class="mz-productlist {% block module-classes %}{% endblock module-classes %} dj-product-list">
    {% if model.items %}
    <ul class="mz-productlist-list {% block list-classes %}{% endblock list-classes %} dj-tiles">
        {% for prod in model.items %}
            <li class="mz-productlist-item" data-mz-product="{{ prod.productCode }}">{% include "modules/product/product-listing" with model=prod %}</li>
        {% endfor %}
    </ul>
    {% endif %}
</div>
