<div class="sitenav-promotion">
	{% if not user.isAnonymous %}
		{% if Model.config.promoHTML %}
			{{ Model.config.promoHTML|escape }}
		{% else %}
			<div class="featured-ad">
				<a href="{{ Model.config.promoURL }}"><img src="{{ Model.config.imageURL }}" /></a>
			</div>
		{% endif %}
	{% else %}
		<div class="sign-in-cta">
			<a href="#">{{ labels.signIn1 }}</a>
			<p>{{ labels.signIn2 }}</p>
		</div>
	{% endif %}
</div>