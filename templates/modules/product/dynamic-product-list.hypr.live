{% for prod in model.items %}
	<div data-mz-product="{{prod.productCode}}">
		<a href="{{prod.url}}">
			<div class="image-container"><img class="dycon-img" src="{{prod.mainImage.imageUrl}}?max={% block thumbnail-size %}{{ themeSettings.listProductThumbSize }}{% endblock thumbnail-size %}" {% if prod.mainImage.altText %}alt="{{ prod.mainImage.altText }}"{% endif %}/></div>
		</a>
		<a class="product-name" href="{{prod.url}}">{{prod.content.productName}}</a>
	</div>
{% endfor %}